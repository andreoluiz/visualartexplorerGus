{
    "version": 2,
    "builds": [
      {
        "src": "api-main/main.go",
        "use": "@vercel/go"
      },
      {
        "src": "frontend/package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "frontend/dist" 
        }
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "api-main/main.go"
      },
      {
        "src": "/(.*)",
        "dest": "frontend/dist/$1"
      }
    ]
  }
  